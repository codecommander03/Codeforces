#include <bits/stdc++.h>
using namespace std;
 
void solve(){
    int n;
    cin>>n;
    int odd=0,even=0;
    while(n--){
        int x;
        cin>>x;
        if(x%2) odd+=x;
        else even+=x;
    }
    if(even>odd) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int t=-1;
    cin>>t;
    while(t--) solve();
}
