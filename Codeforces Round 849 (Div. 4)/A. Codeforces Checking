#include <bits/stdc++.h>
 
using namespace std;
void solve(){
    char c;
    cin>>c;
    bool flag = false;
    string s = "codeforces";
    int n = s.size();
    for(int i=0;i<n;i++){
        if(s[i]==c){
            flag = true;
            break;
        }
    }
    if (flag) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
}
 
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int t=-1;
    cin>>t;
    while(t--) solve();
}
